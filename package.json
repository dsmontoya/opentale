{
  "name": "opentale",
  "productName": "opentale",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "test": "jest --config src/jest.config.js",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "daniel",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "opentale"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.9.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "fountain-converter": "^1.1.4",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "source-map-support": "^0.5.13",
    "ts-jest": "^24.0.2",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vueify": "^9.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@electron-forge/cli": "^6.0.0-beta.44",
    "@electron-forge/maker-deb": "^6.0.0-beta.44",
    "@electron-forge/maker-rpm": "^6.0.0-beta.44",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.44",
    "@electron-forge/maker-zip": "^6.0.0-beta.44",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.44",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "electron": "6.0.4",
    "jest": "^24.7.1",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10"
  }
}
